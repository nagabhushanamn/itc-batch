package com;

class Person{
	String name;
	void sayName(){
		String name="LOCAL";
		System.out.println("im "+name);
		System.out.println("im "+this.name);
	}
}


public class Demo {

	public static void main(String[] args) {
		
		Person person=new Person();
		person.name="Nag";
		
		person.sayName();
		
	}
	
}
